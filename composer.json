{
  "name": "kslimeromk/slim-mvc-starter",
  "description": "Slim 4 MVC Starter Kit with Eloquent and Laravel Migrations",
  "authors": [
    {
      "name": "Zoran Shefot Bogoevski",
      "email": "zbogoevski@gmail.com"
    }
  ],
  "type": "project",
  "require": {
    "php": ">=8.2",
    "bryanjhv/slim-session": "~4.0",
    "firebase/php-jwt": "^6.11",
    "illuminate/config": "^v12.3.0",
    "illuminate/database": "^12.0",
    "illuminate/view": "^12.0",
    "php-di/slim-bridge": "^3.4",
    "phpmailer/phpmailer": "^6.9",
    "slim/csrf": "^1.5",
    "slim/http": "^1.3",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.0",
    "symfony/console": "^6.0",
    "vlucas/phpdotenv": "^5.6",
    "zeuxisoo/slim-whoops": "0.7.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Support\\": "support",
      "Database\\Migrations\\": "database/migrations/"
    },
    "files": [
      "support/BladeViewHelper.php",
      "support/BladeAssetHelper.php"

    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "laravel/pint": "^1.21",
    "rector/rector": "2.0.9"

  }
}
